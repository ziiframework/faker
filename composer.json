{
    "name": "ziiframework/faker",
    "description": "zii-framework faker",
    "keywords": ["zii-framework", "faker"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/ziiframework/faker/issues",
        "source": "https://github.com/ziiframework/faker"
    },
    "authors": [
        {
            "name": "Yii Team",
            "email": "team@yiiframework.com",
            "homepage": "https://www.yiiframework.com/team"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4.0",
        "ziiframework/zii": ">=3.5.23",
        "fakerphp/faker": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "yii\\faker\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\faker\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "ziiframework/composer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "scripts": {
        "redo-install": [
            "rm -rf vendor/",
            "rm -f composer.lock",
            "@composer self-update -vvv",
            "@composer install -vvv"
        ]
    }
}
